{
  "name": "functionaljs",
  "version": "1.0.0",
  "description": "A repo for learning functional concepts in javascript",
  "main": "main.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ocalvet/functional-js.git"
  },
  "keywords": [
    "functional",
    "javascript"
  ],
  "author": "Ovidio R. Calvet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ocalvet/functional-js/issues"
  },
  "homepage": "https://github.com/ocalvet/functional-js#readme",
  "devDependencies": {
    "jest": "^23.6.0"
  },
  "jest": {
    "coverageReporters": [
      "html",
      "json-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 50,
        "lines": 50,
        "statements": 80
      }
    }
  },
  "dependencies": {
    "folktale": "^2.3.1",
    "jest-coverage-badges": "^1.0.0",
    "ramda": "^0.25.0",
    "sanctuary": "^0.15.0"
  }
}
